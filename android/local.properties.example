## Template for local.properties
## Copy this file to local.properties and fill in your actual values.
## DO NOT commit local.properties to version control.

# Android SDK path (auto-generated by Flutter)
sdk.dir=/path/to/android/sdk
flutter.sdk=/path/to/flutter/sdk

# Google Maps API Key (required for map features)
# Get your key from: https://console.cloud.google.com/google/maps-apis
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE
